<p id="notice"><%= notice %></p>

<h1>Settings</h1>

<table>
  <thead>
    <tr>
      <th>Discipline full</th>
      <th>Discipline extramural</th>
      <th>Training direction full</th>
      <th>Training direction extramural</th>
      <th>Course full</th>
      <th>Course extramural</th>
      <th>Contingent full</th>
      <th>Contingent extramural</th>
      <th>Subgroups full</th>
      <th>Subgroups extramural</th>
      <th>Semester full</th>
      <th>Lectures full</th>
      <th>Lectures extramural</th>
      <th>Practical classes full</th>
      <th>Practical classes extramural</th>
      <th>Laboratory classes full</th>
      <th>Laboratory classes extramural</th>
      <th>Modular control full</th>
      <th>Consultation full</th>
      <th>Consultation extramural</th>
      <th>Test plan full</th>
      <th>Test plan extramural</th>
      <th>Test hours full</th>
      <th>Test hours extramural</th>
      <th>Exam full</th>
      <th>Exam extramural</th>
      <th>Exam v extramural</th>
      <th>Exam w extramural</th>
      <th>Work plan full</th>
      <th>Work plan extramural</th>
      <th>Budget hours full</th>
      <th>Budget hours</th>
      <th>Contract hours full</th>
      <th>Contract hours extramural</th>
      <th>Department</th>
      <th>Years</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @settings.each do |setting| %>
      <tr>
        <td><%= setting.discipline_full %></td>
        <td><%= setting.discipline_extramural %></td>
        <td><%= setting.training_direction_full %></td>
        <td><%= setting.training_direction_extramural %></td>
        <td><%= setting.course_full %></td>
        <td><%= setting.course_extramural %></td>
        <td><%= setting.contingent_full %></td>
        <td><%= setting.contingent_extramural %></td>
        <td><%= setting.subgroups_full %></td>
        <td><%= setting.subgroups_extramural %></td>
        <td><%= setting.semester_full %></td>
        <td><%= setting.lectures_full %></td>
        <td><%= setting.lectures_extramural %></td>
        <td><%= setting.practical_classes_full %></td>
        <td><%= setting.practical_classes_extramural %></td>
        <td><%= setting.laboratory_classes_full %></td>
        <td><%= setting.laboratory_classes_extramural %></td>
        <td><%= setting.modular_control_full %></td>
        <td><%= setting.consultation_full %></td>
        <td><%= setting.consultation_extramural %></td>
        <td><%= setting.test_plan_full %></td>
        <td><%= setting.test_plan_extramural %></td>
        <td><%= setting.test_hours_full %></td>
        <td><%= setting.test_hours_extramural %></td>
        <td><%= setting.exam_full %></td>
        <td><%= setting.exam_extramural %></td>
        <td><%= setting.exam_v_extramural %></td>
        <td><%= setting.exam_w_extramural %></td>
        <td><%= setting.work_plan_full %></td>
        <td><%= setting.work_plan_extramural %></td>
        <td><%= setting.budget_hours_full %></td>
        <td><%= setting.budget_hours_extramural%></td>
        <td><%= setting.contract_hours_full %></td>
        <td><%= setting.contract_hours_extramural %></td>
        <td><%= setting.department %></td>
        <td><%= setting.years %></td>
        <td><%= link_to 'Show', setting %></td>
        <td><%= link_to 'Edit', edit_setting_path(setting) %></td>
        <td><%= link_to 'Destroy', setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

